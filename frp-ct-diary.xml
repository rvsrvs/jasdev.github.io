---
layout: null
---

<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

 <title>Jasdev Singh - F(R)P and CT Diary</title>
 <link href="{{ site.url }}{{ "/frp-ct-diary.xml" | relative_url }}" rel="self"/>
 <link href="{{ site.url }}{{ "/frp-ct-diary" | relative_url }}"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>{{ site.url }}</id>
 <author>
   <name>{{ site.author.name }}</name>
   <email>{{ site.author.email }}</email>
 </author>

 {% assign sorted_diary_entries = site.frp-ct-diary | sort: "date" | reverse %}
 {% for entry in sorted_diary_entries %}
 {% if entry.title != "F(R)P and CT Diary Archive" %}
 <entry>
   <title>{{ entry.title }}</title>
   <link href="{{ site.url }}{{ thought.url }}"/>
   <updated>{{ entry.date | date_to_xmlschema }}</updated>
   <id>{{ site.url }}{{ entry.url }}</id>
   <content type="html">{{ entry.content | xml_escape }}</content>
 </entry>
 {% endif %}
 {% endfor %}

</feed>
